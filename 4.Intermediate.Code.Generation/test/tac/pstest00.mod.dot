digraph IR {
  graph [fontname="Times New Roman",fontsize=10];
  node  [fontname="Courier New",fontsize=10];
  edge  [fontname="Times New Roman",fontsize=10];

  // scope 'pstest00'
  pstest00_cb [label="pstest00\r  0:     call   input\l  1:     call   process\l  2:     call   output\l",shape=box]
  // scope 'input'
  input_cb [label="input\r  0:     call   t0 <- ReadInt\l  1:     assign n <- t0\l  2:     call   t1 <- ReadInt\l  3:     assign m <- t1\l  4:     assign i <- 0\l  5:     assign j <- 0\l  6: 5_while_cond:\l  7:     mul    t2 <- n, m\l  8:     if     i < t2 goto 6_while_body\l  9:     goto   4\l 10: 6_while_body:\l 11:     call   t3 <- ReadInt\l 12:     &()    t4 <- a\l 13:     mul    t5 <- i, 4\l 14:     &()    t6 <- a\l 15:     param  0 <- t6\l 16:     call   t7 <- DOFS\l 17:     add    t8 <- t5, t7\l 18:     add    t9 <- t4, t8\l 19:     assign @t9 <- t3\l 20:     add    t10 <- i, 1\l 21:     assign i <- t10\l 22:     goto   5_while_cond\l 23: 4:\l 24:     assign i <- 0\l 25: 12_while_cond:\l 26:     if     i < n goto 13_while_body\l 27:     goto   11\l 28: 13_while_body:\l 29: 16_while_cond:\l 30:     if     j < m goto 17_while_body\l 31:     goto   15\l 32: 17_while_body:\l 33:     call   t11 <- ReadInt\l 34:     &()    t12 <- b\l 35:     param  1 <- 2\l 36:     &()    t13 <- b\l 37:     param  0 <- t13\l 38:     call   t14 <- DIM\l 39:     mul    t15 <- i, t14\l 40:     add    t16 <- t15, j\l 41:     mul    t17 <- t16, 4\l 42:     &()    t18 <- b\l 43:     param  0 <- t18\l 44:     call   t19 <- DOFS\l 45:     add    t20 <- t17, t19\l 46:     add    t21 <- t12, t20\l 47:     assign @t21 <- t11\l 48:     add    t22 <- j, 1\l 49:     assign j <- t22\l 50:     goto   16_while_cond\l 51: 15:\l 52:     add    t23 <- i, 1\l 53:     assign i <- t23\l 54:     goto   12_while_cond\l 55: 11:\l 56:     return \l",shape=box]
  // scope 'div'
  div_cb [label="div\r  0:     div    t0 <- value, q\l  1:     return t0\l",shape=box]
  // scope 'mod'
  mod_cb [label="mod\r  0:     div    t0 <- value, q\l  1:     mul    t1 <- t0, q\l  2:     sub    t2 <- value, t1\l  3:     return t2\l",shape=box]
  // scope 'pos'
  pos_cb [label="pos\r  0:     mul    t0 <- n, m\l  1:     param  1 <- t0\l  2:     param  0 <- index\l  3:     call   t1 <- mod\l  4:     assign x <- t1\l  5:     param  1 <- m\l  6:     param  0 <- x\l  7:     call   t2 <- div\l  8:     param  1 <- 2\l  9:     param  0 <- arr\l 10:     call   t3 <- DIM\l 11:     mul    t4 <- t2, t3\l 12:     param  1 <- m\l 13:     param  0 <- x\l 14:     call   t5 <- mod\l 15:     add    t6 <- t4, t5\l 16:     mul    t7 <- t6, 4\l 17:     param  0 <- arr\l 18:     call   t8 <- DOFS\l 19:     add    t9 <- t7, t8\l 20:     add    t10 <- arr, t9\l 21:     if     @t10 = 0 goto 2_if_true\l 22:     goto   3_if_false\l 23: 2_if_true:\l 24:     return 0\l 25:     goto   1\l 26: 3_if_false:\l 27: 1:\l 28:     return 1\l",shape=box]
  // scope 'process'
  process_cb [label="process\r  0:     assign i <- 0\l  1: 2_while_cond:\l  2:     &()    t0 <- b\l  3:     param  1 <- t0\l  4:     &()    t1 <- a\l  5:     mul    t2 <- i, 4\l  6:     &()    t3 <- a\l  7:     param  0 <- t3\l  8:     call   t4 <- DOFS\l  9:     add    t5 <- t2, t4\l 10:     add    t6 <- t1, t5\l 11:     param  0 <- @t6\l 12:     call   t7 <- pos\l 13:     if     t7 = 1 goto 4\l 14:     goto   1\l 15: 4:\l 16:     mul    t8 <- n, m\l 17:     if     i < t8 goto 3_while_body\l 18:     goto   1\l 19: 3_while_body:\l 20:     &()    t9 <- a\l 21:     mul    t10 <- i, 4\l 22:     &()    t11 <- a\l 23:     param  0 <- t11\l 24:     call   t12 <- DOFS\l 25:     add    t13 <- t10, t12\l 26:     add    t14 <- t9, t13\l 27:     add    t15 <- sum, @t14\l 28:     assign sum <- t15\l 29:     add    t16 <- i, 1\l 30:     assign i <- t16\l 31:     goto   2_while_cond\l 32: 1:\l",shape=box]
  // scope 'output'
  output_cb [label="output\r  0:     param  0 <- sum\l  1:     call   WriteInt\l",shape=box]
};
